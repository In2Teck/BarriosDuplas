<h1>Listing additional_badges</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @additional_badges.each do |additional_badge| %>
  <tr>
    <td><%= additional_badge.name %></td>
    <td><%= link_to 'Show', additional_badge %></td>
    <td><%= link_to 'Edit', edit_additional_badge_path(additional_badge) %></td>
    <td><%= link_to 'Destroy', additional_badge, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Additional badge', new_additional_badge_path %>
